# Default key
# -----------------------------------------------------------------------------
keymap C-g  LINE_INFO
keymap C-k  COOKIE
keymap C-l  REDRAW
keymap C-q  CLOSE_TAB

keymap C-u  PREV_LINK
keymap M-TAB PREV_LINK
keymap M-[Z  PREV_LINK

keymap C-w  WRAP_TOGGLE
keymap C-z  SUSPEND

keymap  !   SHELL
keymap  #   PIPE_SHELL
keymap  @   READ_SHELL

keymap  ,   LEFT
keymap  .   RIGHT

keymap  /   SEARCH
keymap  ?   SEARCH_BACK

keymap  =   INFO

keymap  <   SHIFT_LEFT
keymap  >   SHIFT_RIGHT

keymap  D   DOWNLOAD_LIST

keymap  E   EDIT

keymap  F   FRAME

keymap  v   VIEW
keymap  I   VIEW_IMAGE

keymap  L   LIST

keymap  M   EXTERN

keymap  Q   EXIT

keymap  R   RELOAD

keymap  S   SAVE_SCREEN
keymap  M-e EDIT_SCREEN

keymap  T   NEW_TAB

keymap  V   LOAD

keymap  Z   CENTER_H

keymap  [   LINK_BEGIN
keymap  ]   LINK_END

keymap  a   SAVE_LINK

keymap  c   PEEK
keymap  i   PEEK_IMG
keymap  u   PEEK_LINK

keymap  m   MOUSE_TOGGLE

keymap  o   OPTIONS

keymap  r   VERSION

keymap  |   PIPE_BUF

keymap  M-C-j   SAVE_LINK
keymap  M-C-m   SAVE_LINK

keymap  M-I SAVE_IMAGE
keymap  M-M EXTERN_LINK
keymap  M-W DICT_WORD_AT
keymap  M-c COMMAND

keymap  M-g GOTO_LINE
keymap  M-k DEFINE_KEY
keymap  M-o SET_OPTION
keymap  M-u GOTO_RELATIVE
keymap  M-w DICT_WORD

keymap  M-[E    MENU
keymap  M-[L    MENU
keymap  M-[2~   MENU
keymap  M-[28~  MENU
keymap  M-t TAB_MENU
keymap  M-l LIST_MENU
keymap  s   SELECT_MENU
keymap  M-m MOVE_LIST_MENU

keymap M-b  BOOKMARK
keymap M-a  ADD_BOOKMARK

keymap C-h  HISTORY

# Mark
# -----------------------------------------------------------------------------
# keymap C-@  MARK
# keymap :    MARK_URL
# keymap ";"  MARK_WORD
# keymap M-n  NEXT_MARK
# keymap M-p  PREV_MARK
# keymap M-:  MARK_MID
# keymap \"   REG_MARK
keymap C-@  NULL
keymap :    NULL
keymap ";"  NULL
keymap M-n  NULL
keymap M-p  NULL
keymap M-:  NULL
keymap \"   NULL

# Line movement
# -----------------------------------------------------------------------------
keymap 0    LINE_BEGIN
keymap \^   LINE_BEGIN
keymap $    LINE_END

keymap k    MOVE_UP
keymap j    MOVE_DOWN

keymap h    MOVE_LEFT
keymap l    MOVE_RIGHT

keymap C-y  UP
keymap C-e  DOWN

keymap C-d  NEXT_PAGE
keymap C-f  NEXT_PAGE
keymap C-u  PREV_PAGE
keymap C-b  PREV_PAGE

keymap gg   BEGIN
keymap G    END

keymap w    NEXT_WORD
keymap b    PREV_WORD

keymap zz   CENTER_V

# Search
# -----------------------------------------------------------------------------
keymap /    ISEARCH
keymap ?    ISEARCH_BACK

keymap N    SEARCH_PREV
keymap n    SEARCH_NEXT

# Link and Tab
# -----------------------------------------------------------------------------
keymap C-]  GOTO_LINK
keymap TAB  NEXT_LINK

keymap :t   TAB_LINK
keymap C-t  BACK

keymap gT   PREV_TAB
keymap gt   NEXT_TAB

# Miscellaneous
# -----------------------------------------------------------------------------
keymap u    UNDO
keymap C-r  REDO

keymap q    QUIT
keymap :q   QUIT
keymap :w   SAVE
keymap :e   GOTO
keymap :h   HELP
