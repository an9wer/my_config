# Reset to NULL
# -----------------------------------------------------------------------------
# keymap h    MOVE_LEFT
# keymap C-b  MOVE_LEFT
# keymap LEFT MOVE_LEFT
# keymap C-f  MOVE_RIGHT
# keymap l    MOVE_RIGHT
# keymap RIGHT MOVE_RIGHT
keymap l     NULL
keymap C-b   NULL
keymap LEFT  NULL
keymap h     NULL
keymap C-f   NULL
keymap RIGHT NULL

# keymap k    MOVE_UP
# keymap C-p  MOVE_UP
# keymap UP   MOVE_UP
# keymap j    MOVE_DOWN
# keymap C-n  MOVE_DOWN
# keymap DOWN MOVE_DOWN
keymap k    NULL
keymap C-p  NULL
keymap UP   NULL
keymap j    NULL
keymap C-n  NULL
keymap DOWN NULL

# keymap  \^  LINE_BEGIN
# keymap C-a  LINE_BEGIN
# keymap C-e  LINE_END
# keymap  $   LINE_END
keymap  \^  NULL
keymap C-a  NULL
keymap C-e  NULL
keymap $    NULL

# keymap J    UP
# keymap K    DOWN
keymap J    NULL
keymap K    NULL

# keymap C-v     NEXT_PAGE
